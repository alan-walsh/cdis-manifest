  "components": {
    "appName": "ARDaC: AlcHepNet Research Data Commons",
    "index": {
      "introduction": {
        "heading": "ARDaC: AlcHepNet Research Data Commons",
        "text": "The AlcHepNet Research Data Commons (ARDaC) supports the management, analysis, and sharing of clinical data to facilitate investigations of the epidemiology, diagnosis, pathophysiology, natural history, and treatment of alcoholic hepatitis.",
        "buttonText": "Submit/Browse Data",
        "link": "/submission"
      },
      "buttons": [
          {
            "name": "Explore Data",
            "icon": "data-explore",
            "body": "Use the Exploration tool to browse ARDaC datasets.",
            "link": "/explorer",
            "label": "Explore Data"
          },
          {
            "name": "Data Dictionary",
            "icon": "data-field-define",
            "body": "ARDaC employs a data model to power queries and data submissions. Study the dictionary to send GraphQL queries or format your data submissions.",
            "link": "/DD",
            "label": "View Dictionary"
          },
          {
            "name": "Query Data",
            "icon": "data-access",
            "body": "Search and download subsets of data from ARDaC using GraphQL queries.",
            "link": "/query",
            "label": "Query Data"
          },
          {
            "name": "Discover Study Metadata",
            "icon": "data-explore",
            "body": "Discover study descriptions and other metadata stored in ARDaC",
            "link": "/discovery",
            "label": "Discover Study Metadata"
          },
            {
            "name": "Analyze Data",
            "icon": "data-analyze",
            "body": "Perform analysis on ARDaC data using Jupyter Notebooks.",
            "link": "/workspace",
            "label": "Run Analysis"
          }
      ]
    },
    "navigation": {
      "items": [
        {
          "icon": "query",
          "link": "/discovery",
          "name": "Discovery",
          "tooltip": "Use free-text search and tags to rapidly find relevant studies."
        },
        {
          "icon": "dictionary",
          "link": "/DD",
          "color": "#a2a2a2",
          "name": "Dictionary",
          "tooltip": "The Data Dictionary serves to inform the data model and is updated as new data is ingested."
        },
        {
          "icon": "exploration",
          "link": "/explorer",
          "color": "#a2a2a2",
          "name": "Exploration",
          "tooltip": "The Exploration Page enables discovery of the data at the subject level and features a cohort builder."
        },
        {
          "icon": "query",
          "link": "/query",
          "color": "#a2a2a2",
          "name": "Query",
          "tooltip": "Query the databases using GraphiQL."
        },
        {
          "name": "Notebook Browser",
          "link": "/resource-browser",
          "icon": "analysis",
          "tooltip": "Learn how to use Jupyter Notebooks to explore and visualize data available by running a tutorial notebook, or use one of these examples as a launching point for your own custom analysis."
        },
        {
          "icon": "workspace",
          "link": "#hostname#workspace/",
          "color": "#a2a2a2",
          "name": "Workspace",
          "tooltip": "Gen3 workspaces are secure data analysis environments in the cloud that can access data from one or more data resources."
        },
        {
          "icon": "profile",
          "link": "/identity",
          "color": "#a2a2a2",
          "name": "Profile",
          "tooltip": "Create API keys for programmatic data access, and review your authorization privileges to datasets and services."
        }
      ]
    },
    "topBar": {
      "items": [
        {
          "icon": "upload",
          "link": "/submission",
          "name": "Submit Data"
        },
        {
          "link": "https://gen3.org/resources/user/",
          "name": "Documentation"
        }
      ]
    },
    "login": {
      "title": "ARDaC: AlcHepNet Research Data Commons",
      "subTitle": "Explore, Analyze, and Share Data",
      "text": "The AlcHepNet Research Data Commons (ARDaC) supports the management, analysis, and sharing of clinical data to facilitate investigations of the epidemiology, diagnosis, pathophysiology, natural history, and treatment of alcoholic hepatitis.",
      "contact": "If you have any questions about access or the registration process, please contact ",
      "email": "su1@iu.edu",
